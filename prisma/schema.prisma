// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Card {
  id        String @id @default(cuid())
  name      String
  src       String
  text      String
  alt       String
  nameColor String
  href      String
  routeHref String
}
